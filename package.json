{
  "name": "pubstar",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:next": "i18nexus pull --path src/locales && next dev -p 15889",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "codegen": "openapi-generator-cli generate -i ./swagger.json -g typescript-axios -o src/services/openapi -t .openapi-generator/templates",
    "watch:codegen": "nodemon --watch ./swagger.json --exec npm run codegen",
    "dev": "concurrently \"yarn watch:codegen\" \"yarn dev:next\""
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.5",
    "@typescript-eslint/eslint-plugin": "^7.12.0",
    "antd": "^5.18.0",
    "axios": "^1.7.2",
    "dotenv": "^16.4.5",
    "dotenv-webpack": "^8.1.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "highcharts": "^11.4.3",
    "highcharts-react-official": "^3.2.1",
    "i18next": "^23.11.5",
    "i18next-resources-to-backend": "^1.2.1",
    "jose": "^5.4.0",
    "next": "14.2.3",
    "next-i18n-router": "^5.5.0",
    "papaparse": "^5.4.1",
    "react": "^18",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18",
    "react-google-recaptcha": "^3.1.0",
    "react-i18next": "^14.1.2",
    "react-redux": "^9.1.2",
    "react-resizable": "^3.0.5",
    "sass": "^1.77.5",
    "xlsx-populate": "^1.21.0"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.13.4",
    "@types/node": "20.14.2",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "i18nexus-cli": "^3.4.0",
    "nodemon": "^3.1.3",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.4",
    "typescript": "5.4.5"
  }
}
